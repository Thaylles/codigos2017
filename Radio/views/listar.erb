<!DOCTYPE html>
<html>
<head>
	<title></title>
</head>
<body>

<div class="container">
<div class="row">
<!-- code start -->
<% @vet.each do |comunicador| %>
<div class="twPc-div">
    <a class="twPc-bg twPc-block"></a>
	<div>
		<div class="twPc-button">
	    <button class="btn-warming" onclick="window.location.href='/admin/tela_alterar/<%= comunicador.id %>'">Alternar</button>
	    <button class="btn-danger" onclick="window.location.href='/admin/excluir/<%= comunicador.id %>'">Excluir</button>
	</div>
	<img src= "<%= comunicador.foto %>" class="twPc-avatarImg">
		<div class="twPc-divUser">
			<div class="twPc-divName">
				<a href="https://twitter.com/mertskaplan"><%= comunicador.nome %></a>
			</div>
		</div>
	</div>
</div>
<br>
<br>
<% end %>
<button type="button" class="btn-warning" id="desmuda">Mudar Fundo</button>
</div>
</div>
</body>
</html>
